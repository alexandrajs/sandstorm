{
  "name": "sandstorm",
  "version": "0.3.12",
  "description": "Not a MongoDB ORM",
  "main": "./index.js",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "raw-test": "node_modules/.bin/_mocha",
    "docs": "./node_modules/.bin/jsdoc -c ./.jsdocrc -r -R ./README.md  -d ../../gh-pages/sandstorm/ src/",
    "codeclimate": "docker run --rm --env CODECLIMATE_CODE=\"$PWD\" --volume \"$PWD\":/code --volume /var/run/docker.sock:/var/run/docker.sock --volume /tmp/cc:/tmp/cc codeclimate/codeclimate analyze -f html > codeclimate.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandrajs/sandstorm"
  },
  "keywords": [
    "mongodb",
    "nodejs",
    "orm"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "ponury.kostek@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexandrajs/sandstorm/issues"
  },
  "homepage": "https://github.com/alexandrajs/sandstorm#readme",
  "peerDependencies": {
    "mongodb": "^3.1"
  },
  "dependencies": {
    "amule": "^0.2.5",
    "amule-aim": "^0.5.3",
    "amule-more": "^0.2.1",
    "amule-rush": "^0.3.5",
    "bluebird": "^3.5.2",
    "exterror": "^1.1.0",
    "fast.js": "^0.1.1",
    "run-parallel": "^1.1.9"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.2",
    "ink-docstrap": "^1.3.2",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mongodb": "^3.1.8"
  }
}
